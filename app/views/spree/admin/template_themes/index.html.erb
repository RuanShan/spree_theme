<h1>Listing themes</h1>

<table>
  <tr>
    <th>TemplateTheme</th>
  </tr>

<% @themes.each do |theme| %>
  <tr>
    <td><%= theme.title %></td>

    <td><%= link_to 'Destroy', admin_template_theme_path(theme), :confirm => 'Are you sure?', :method => :delete %></td>
    <td><%= link_to 'copy', copy_template_theme_path(theme), {:method=>:post} %></td>
    <td><%= link_to 'Editor', editor_template_theme_path(theme) %></td>
    <td><%= link_to 'Build', build_template_theme_path(theme) %></td>
    <td><%= link_to 'GenerateAssets', generate_assets_template_theme_path(theme)   %></td>
  </tr>
<% end %>
</table>

<br />

